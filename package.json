    "build": "pnpm with-env tsx server/db/migrate && pnpm with-env next build",
    "start": "pnpm with-env next start",
    "lint": "next lint && biome lint --write --unsafe",
    "lint:fix": "next lint --fix && biome lint --write --unsafe",
    "format": "biome format --write",
    "db:generate": "pnpm with-env drizzle-kit generate",
    "db:migrate": "pnpm with-env tsx server/db/migrate.ts",
    "db:studio": "pnpm with-env drizzle-kit studio",
    "db:push": "pnpm with-env drizzle-kit push",
    "db:pull": "pnpm with-env drizzle-kit pull",
    "db:check": "pnpm with-env drizzle-kit check",
    "db:up": "pnpm with-env drizzle-kit up",
    "test": "export PLAYWRIGHT=True && pnpm with-env pnpm exec playwright test",
    "with-env": "dotenv -e ./.env --"
  },
